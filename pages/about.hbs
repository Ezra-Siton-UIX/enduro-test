{{>head}}
<!--plugins-->
{{>fancybox-css}}

<style>
/* =========== about-page ================ */
/* TAB */
@media screen and (max-width: 900px) {
	.uk-tab .uk-text-large{
		text-align: center;
		font-size: 15px;
	}
}
@media screen and (min-width: 900px) {
	.uk-tab li a:hover{
		background-color: rgba(51, 51, 51, 0.05);
	}
}
.uk-accordion-title{
	background: white;
	padding: 15px 25px;
	border-bottom: 1px solid  rgba(51, 51, 51, 0.1);
}
.uk-accordion-title:hover{
	background: rgba(51, 51, 51, 0.01);
}
.uk-accordion-content{
	margin-top: 0px;
	padding: 0px 25px 25px 5px;
}

aside ul li span{

	margin-left: 1px;
}

.item:not(:first-child) {
	border-top: 1px solid #e5e5e5;
}
</style>

</head>

<body itemscope itemtype="http://schema.org/AboutPage" style="background: #f8f8f8;">
	{{#>body}}



	{{>inside_header}}


	<!-- Breadcrumb -->
	<nav class="uk-container uk-container-large uk-margin-bottom" itemprop="breadcrumb">
		<ol itemscope itemtype="http://schema.org/BreadcrumbList" class="uk-breadcrumb uk-margin-small-top">
			<li itemprop="itemListElement" itemscope	itemtype="http://schema.org/ListItem">
				<a itemscope itemtype="http://schema.org/Thing" itemprop="item"  href="../{{global.language}}" data-uk-tooltip="pos: bottom" title="{{global.breadcrumbs_back_to_main}}">
					<svg xmlns="http://www.w3.org/2000/svg" width="14" height="14" viewbox="0 0 24 24">
						<g class="nc-icon-wrapper" fill="#999999">
							<path d="M10 20v-6h4v6h5v-8h3L12 3 2 12h3v8z"></path>
						</g>
					</svg>
					<span itemprop="name">{{global.home_label_name}}</span>
				</a>
				<meta itemprop="position" content="1" />
			</li>
			<li itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
				<a itemscope itemtype="http://schema.org/Thing" itemprop="item" href="../{{global.language}}/{{page_name}}"> <span itemprop="name">{{seo.title}}</span></a>
				<meta itemprop="position" content="2" />
			</li>
		</ol>
	</nav>

	<section class="uk-section uk-section-default uk-section-l2arge">
