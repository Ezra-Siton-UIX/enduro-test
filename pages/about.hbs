	<section class="uk-background-norepeat uk-background-cover uk-background-center-center uk-background-image@m uk-flex uk-flex-middle" data-uk-height-viewport="offset-top: true" data-uk-parallax="bgy: 100; viewport:0.5" style="background-image: url('{{isometric.bgImg}}');box-sizing: border-box; min-height: 100vh;">
		<div class="uk-width-1-1">
